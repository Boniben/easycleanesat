{% extends 'base.html.twig' %}

{% block title %}Type de zone index{% endblock %}

{% block body %}
    <h1>Type de zone index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Picto</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_zone in type_zones %}
            <tr>
                <td>{{ type_zone.id }}</td>
                <td>{{ type_zone.nom }}</td>
                <td>{{ type_zone.picto }}</td>
                <td>
                    <a href="{{ path('app_type_zone_show', {'id': type_zone.id}) }}">show</a>
                    <a href="{{ path('app_type_zone_edit', {'id': type_zone.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_zone_new') }}">Create new</a>
{% endblock %}
