<form class="delete-inline" method="post" action="{{ path('app_client_delete', {'id': client.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce client ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ client.id) }}">
	<button type="submit" class="btn btn-danger btn-sm">Supprimer</button>
</form>
