{% extends 'base.html.twig' %}

{% block title %}Contenant{% endblock %}

{% block body %}
    <h1>Contenant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contenant.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ contenant.nom }}</td>
            </tr>
            <tr>
                <th>VolumeEau</th>
                <td>{{ contenant.volumeEau }}</td>
            </tr>
            <tr>
                <th>Picto</th>
                <td>{{ contenant.picto }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_contenant_index') }}">Retour a la liste</a>

    <a href="{{ path('app_contenant_edit', {'id': contenant.id}) }}">Modifier</a>

    {{ include('contenant/_delete_form.html.twig') }}
{% endblock %}
